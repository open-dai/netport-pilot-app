<div class="map-container" style="height:260px;"></div>
<h3>Skapa rapport</h3>
<form role="form" enctype="multipart/form-data">
    <div class="form-group">
        <label for="typeid">VÃ¤lj kategori</label>
        <select id="type" class="form-control typeid">
            {{#each types}}
                <option value="{{id}}">{{title}}</option>
            {{/each}}
        </select>
    </div>
    <div class="form-group">
        <label for="description">Beskrivning</label>
